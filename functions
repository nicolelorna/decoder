def hello():
  print("Hello AkiraChix")
  
hello()

def hello(name):
  print("Hello {}".format(name))
hello("AkiraChix")


def year_of_birth(name,age):
  year=2020-age
  print("Hello {} you were born in year {}".format(name,year))
year_of_birth("nicole",20)

